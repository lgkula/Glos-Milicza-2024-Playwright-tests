        "husky": "^9.0.0",
        "lint-staged": "^15.0.2",
        "prettier": "3.2.5",
        "typescript": "^5.2.2"
    },
    "lint-staged": {
        "**/*": "prettier --write --ignore-unknown"
    },
    "dependencies": {
        "dayjs": "^1.11.10",
        "dotenv": "^16.4.1",
